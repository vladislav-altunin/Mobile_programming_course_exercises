{
  "expo": {
    "name": "spending-tracker",
    "slug": "spending-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "plugins": [
      [
        "expo-dev-launcher",
        {
          "launchModeExperimental": "most-recent"
        }
      ]
    ],
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vlad.spendingtracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "react-native-google-mobile-ads": {
      "ios_app_id": "ca-app-pub-3940256099942544/9214589741"
    },
    "extra": {
      "eas": {
        "projectId": "d195fbc0-bb0f-40b5-9180-9122e71cc81f"
      }
    }
  }
}
